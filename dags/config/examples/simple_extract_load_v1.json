{
  "dag_id": "simple_extract_load_v1",
  "description": "Simple ETL pipeline demonstrating extract and load operations",
  "schedule_interval": "0 2 * * *",
  "start_date": "2024-01-01",
  "catchup": false,
  "tags": ["example", "beginner", "etl"],
  "default_args": {
    "owner": "airflow",
    "retries": 2,
    "retry_delay_minutes": 5,
    "email_on_failure": false,
    "email_on_retry": false
  },
  "tasks": [
    {
      "task_id": "extract_data",
      "operator": "BashOperator",
      "params": {
        "bash_command": "echo 'Extracting data from source...'"
      }
    },
    {
      "task_id": "validate_extraction",
      "operator": "BashOperator",
      "params": {
        "bash_command": "echo 'Validating extracted data...'"
      },
      "dependencies": ["extract_data"]
    },
    {
      "task_id": "load_data",
      "operator": "BashOperator",
      "params": {
        "bash_command": "echo 'Loading data to warehouse...'"
      },
      "dependencies": ["validate_extraction"]
    },
    {
      "task_id": "verify_load",
      "operator": "BashOperator",
      "params": {
        "bash_command": "echo 'Verifying data load complete'"
      },
      "dependencies": ["load_data"]
    }
  ]
}
